<%- include('partials/header.ejs') %>

<h1>Register </h1>
<div class="reg">


<form method="post" action="/api/register">
<br>
<input class="inpuReg" type="text" name="name" placeholder="Your Name">
<br>
<input class="inpuReg" type="text" name="surname" placeholder="Your Surname" >
<br>
<input class="inpuReg" type="number" name="cellphone" placeholder="Your Cell No">
<br>
<input class="inpuReg" type="email" name="email" placeholder="Your Email">
<br>
<input class="inpuReg" type="password" name="password" placeholder="Password">
<br>
<input class="inpuReg" type="password" name="repassword" placeholder="Confirm Password" >
<br>
<button type="submit">Register</button>

</div>
</form>




